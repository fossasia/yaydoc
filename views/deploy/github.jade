extends ../layout

block content
  script(src="/socket.io/socket.io.js")
  script.
    var data = {
      'email': "#{email}",
      'gitURL': "#{gitURL}",
      'uniqueId': "#{uniqueId}",
      'encryptedToken': "#{token}",
      'username': "#{username}"
    };
  script(src="/scripts/socket-github.js")
  .container
    .col-md-6.col-md-offset-3#main-content
      .progress
        #progress.progress-bar(role='progressbar', aria-valuenow='0', aria-valuemin='0', aria-valuemax='100', style='width:0%')
          span.sr-only
      .logs.pre-scrollable
        h4.console-output-header Console Output
        ul#messages
      .row(style='text-align: center;margin-top: 20px;')
        a.btn.btn-default(id='btnGithubPages' target="_blank") Github Pages